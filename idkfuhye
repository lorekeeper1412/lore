Tab:Button{
    Name = "Worship",
    Description = nil,
    Callback = function()
    -- Made By sister 00de0l


local Players = game:GetService("Players")
local UIS = game:GetService("UserInputService")
local Debris = game:GetService("Debris")


local player = Players.LocalPlayer
local character = player.Character or player.CharacterAdded:Wait()


local tool = Instance.new("Tool")
tool.RequiresHandle = false
tool.Name = "Worship"
tool.Parent = player.Backpack


local anim = Instance.new("Animation")
anim.Name = "oof"
anim.AnimationId = "rbxassetid://182749109"
anim.Parent = tool


local function playSound(id, parent, vol, pitch)
    local s = Instance.new("Sound")
    s.SoundId = id
    s.Volume = vol or 1
    s.Pitch = pitch or 1
    s.Parent = parent or workspace
    s:Play()
    Debris:AddItem(s, 6)
end


local function doWorship()
    if not character then return end
    local hum = character:FindFirstChildOfClass("Humanoid")
    local head = character:FindFirstChild("Head")


    if not hum or not head then return end


    local track = hum:LoadAnimation(anim)
    track:Play()


    game:GetService("Chat"):Chat(head, "PRAISE ALLAH " .. tostring(math.random(1000,3000)) .. " TIMES A DAY!!!! ALLAH AKBAR", Enum.ChatColor.Red)


    task.wait(5)


    for i = 1, 6 do
        local boom = Instance.new("Explosion")
        boom.Position = head.Position + Vector3.new(math.random(-15,15), math.random(0,15), math.random(-15,15))
        boom.BlastRadius = 350
        boom.DestroyJointRadiusPercent = 0.7
        boom.Hit:Connect(function(part, dist)
            if dist <= 20 then
                part.Anchored = false
                part:BreakJoints()
            end
        end)
        boom.Parent = workspace
    end


    playSound("rbxassetid://134854740", head, 1, 1.3)
    playSound("rbxassetid://247893371", head, 2.5, 1)
    playSound("rbxassetid://137994058", head, 1, 1)
    playSound("rbxassetid://165969964", head, 1, 1)


    character:BreakJoints()
end


tool.Equipped:Connect(function()
    local clicked = false


    local conn
    conn = UIS.InputBegan:Connect(function(input, gpe)
        if clicked then return end
        if input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch then
            clicked = true
            conn:Disconnect()
            doWorship()
        end
    end)
end)


